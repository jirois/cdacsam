<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/elicit_graph"
    app:startDestination="@id/partnerListFragment">

    <fragment
        android:id="@+id/partnerListFragment"
        android:name="com.jinncyapps.authenapp.activities.elicitation.fragments.PartnerListFragment"
        android:label="fragment_partner_list"
        tools:layout="@layout/fragment_partner_list" >
        <argument
            android:name="currentClient"
            app:argType="com.jinncyapps.authenapp.data.Client" />
        <action
            android:id="@+id/action_partnerListFragment_to_elicitClientDetails2"
            app:destination="@id/elicitClientDetails" />
    </fragment>
    <fragment
        android:id="@+id/elicitClientDetails"
        android:name="com.jinncyapps.authenapp.activities.elicitation.fragments.ElicitClientDetails"
        android:label="fragment_elicit_client_details"
        tools:layout="@layout/fragment_elicit_client_details" >
        <argument
            android:name="currentClient"
            app:argType="com.jinncyapps.authenapp.data.Client" />
        <action
            android:id="@+id/action_elicitClientDetails_to_partnerListFragment2"
            app:destination="@id/partnerListFragment" />
    </fragment>
</navigation>